<launch>
  <node pkg="nodelet" type="nodelet" name="pcl_manager" args="manager" output="screen" />

  <!-- Run a VoxelGrid filter to clean NaNs and downsample the data -->
  <node pkg="nodelet" type="nodelet" name="pass_through" args="load pcl/PassThrough pcl_manager" output="screen">
    <!-- <remap from="~input" to="/camera/depth/points" /> -->
    <remap from="~input" to="/wombot_gen2proto/seal_cameras/left_camera/depth/color/points" />
    <rosparam>
      filter_field_name: z
      filter_limit_min: 0.05
      filter_limit_max: 0.23
      filter_limit_negative: False
      leaf_size: 0.01
    </rosparam>
  </node>

</launch>